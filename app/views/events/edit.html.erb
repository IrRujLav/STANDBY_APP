<div class="container">
<%= simple_form_for(@event) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :image %>
  <%= f.input :date %>
  <%= f.input :time %>
  <%= f.input :price %>
  <%= f.input :category %>
  <%= f.submit class: "btn btn-outline-warning mt-3 lh-lg w-100" %>
<% end %>
</div>
