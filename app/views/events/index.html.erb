<h1>Events</h1>

<% @events.each do |event| %>

  <div>
    <h2><%= event.name %></h2>
    <p><%= event.venue.location %></p>
    <p><%= event.description %></p>
    <p><%= event.date %></p>
    <p><%= event.price %></p>
  </div>
  <!--TODO: put link to show-->
  <div>
    <%= link_to "Show", event_path(event) %>
  </div>
<% end %>
